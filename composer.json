{
  "name": "semenovdv80/sdvmvc",
  "description": "This is a simple MVC framework",
  "version": "1.0.0",
  "keywords": ["mvc","framework"],
  "homepage": "http://github.com/semenovdv80/SDVmvc",
  "license": "MIT",
  "type": "project",
  "authors": [
    {
      "name": "Dmitriy Semenov",
      "email": "semenovdv80@gmail.com",
      "homepage": "http://github.com/semenovdv80/",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "semenovdv80/framework": "^1.0",
    "vlucas/phpdotenv": "^2.5"
  },
  "require-dev": {
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
